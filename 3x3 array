#include<stdio.h>
int main() 
{
	int i,j,b;
	int x=3;
	int y=3;

	int a[x][y];
	printf("Enter values of array\n");/*----------assigning value of array*/
	for(i=0;i<x;i++)
	{
		for(j=0;j<y;j++)
		{
			printf("enter the value of array at position a[%d][%d]=",i,j);
			scanf("%d",&a[i][j]);
	
		}
	}
	printf("The value of array is:\n");/*----------viewing the array*/
		for(i=0;i<x;i++)
	{
		for(j=0;j<x;j++)
		{
			printf("%d  ",a[i][j]);
		}
		printf("\n");
	}
	return 0;
}
